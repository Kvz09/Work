 /*Анимированная шапка для сайта*/
 
 .main-header {
/* Задаём относительное позиционирование */
position: relative;
/* Размер шапки сайта */
width: 90%;
margin-top:-25px;
height: 75px;
overflow: hidden;
padding: 4px;
/* Фон шапкм сайта */
background: #0000FF;
/* Подключаем анимацию */
animation: main-hed 1s linear;
}
/* Название сайта */
.main-title{
/* Позиционируем абсолютно, относительно блока main-header */

/* Оформляем шрифт */


text-align: center;
text-transform: uppercase;
/* Подключаем анимацию */
animation: main-titl 10s linear;
}
/**/

/* Анимация фона шапки сайта */
@keyframes main-hed {
0% {
/* Начальный цвет */
background: #0000FF;
}
/* Конечный цвет */
100% {
background: #0000FF;
}
}

/* Анимация названия сайта */
@keyframes main-titl{
0% {
/* Полная прозрачность */
opacity: 0;
/* Масштабируется по вертикали и горизрнтали с нуля */
transform: scale(0);
}
100% {
/* Полная непрозрачность */
opacity: 1;
/* Конечный масштаб 1 х 1 */
transform: scale(1);
}
}

/* Анимация описания сайта */
@keyframes main-descript {
0% {
/* Начало анимации в точке внизу шапки сайта */
transform: translateY(6em);
/* Полная прозрачность */
opacity: 0;
}
/* Возрастание непрозрачности */
50% {
opacity: 0.1;
}
75% {
opacity: 0.3;
}
100% {
/* Завершение анимации в точке позиционирования */
transform: translateY(0);
/* Полная непрозрачность */
opacity: 1;
}
}
 
